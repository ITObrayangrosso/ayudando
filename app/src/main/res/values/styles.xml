<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="ImputTextWhite">
        <item name="android:textColor">@color/colorImputTextWhite</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColorHint">@color/colorImputTextWhite</item>
    </style>

    <style name="contenLogin">
        <item name="android:background">@color/colorContenLogin</item>

    </style>

    <style name="bottons">
        <item name="android:background">@color/colorBottons</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="bottonBlue">
        <item name="android:background">@color/colorBottonBlue</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
